{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "member-ordering": [
            false
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "onespace",
                "index-signature": "onespace",
                "parameter": "onespace",
                "property-declaration": "onespace",
                "variable-declaration": "onespace"
            },
            {
                "call-signature": "onespace",
                "index-signature": "onespace",
                "parameter": "onespace",
                "property-declaration": "onespace",
                "variable-declaration": "onespace"
            }
        ],
        "align": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ],
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "array-type": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-trailing-whitespace": [
            false
        ],
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": true
    },
    "rulesDirectory": []
}